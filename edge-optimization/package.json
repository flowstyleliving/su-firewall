{
  "name": "semantic-uncertainty-edge-optimization",
  "version": "1.0.0",
  "description": "Global edge optimization for Semantic Uncertainty API",
  "main": "src/edge-worker.js",
  "scripts": {
    "dev": "wrangler dev --config cloudflare-global-config.toml",
    "build": "webpack --mode production",
    "build:edge": "npm run build && npm run optimize",
    "optimize": "node scripts/optimize-bundle.js",
    "deploy:americas": "wrangler deploy --env americas --config cloudflare-global-config.toml",
    "deploy:europe": "wrangler deploy --env europe --config cloudflare-global-config.toml", 
    "deploy:asia": "wrangler deploy --env asia --config cloudflare-global-config.toml",
    "deploy:global": "npm run deploy:americas && npm run deploy:europe && npm run deploy:asia",
    "test:performance": "node scripts/performance-test.js",
    "monitor": "node scripts/monitoring-dashboard.js",
    "benchmark": "artillery run benchmarks/edge-performance.yml"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "wrangler": "^4.23.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "artillery": "^2.0.3"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "lru-cache": "^10.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "edge-computing",
    "cdn",
    "performance",
    "cloudflare-workers",
    "global-deployment",
    "semantic-uncertainty"
  ]
}