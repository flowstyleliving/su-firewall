# LLM Semantic Uncertainty Evaluation Framework - Environment Variables
# ==================================================================
# Copy this file to .env and fill in your actual API keys

# LLM API Keys (Required for real API integration)
# Get your API keys from:
# - OpenAI: https://platform.openai.com/api-keys  
# - Anthropic: https://console.anthropic.com/
# - Google: https://makersuite.google.com/app/apikey
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...
GOOGLE_API_KEY=AIza...

# Additional LLM APIs (Optional)
MISTRAL_API_KEY=
GROQ_API_KEY=
HUGGING_FACE_API_KEY=

# Semantic Uncertainty Engine Configuration
SEMANTIC_API_URL=http://localhost:3000
SEMANTIC_API_TIMEOUT=10

# Evaluation Framework Settings
DEFAULT_OUTPUT_DIR=results
DEFAULT_MODELS=gpt4,claude3,gemini
ENABLE_MOCK_RESPONSES=true

# Performance and Rate Limiting
MAX_CONCURRENT_REQUESTS=5
REQUEST_DELAY_MS=100
MAX_PROMPT_LENGTH=10000
MAX_OUTPUT_LENGTH=10000

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json
ENABLE_TRACE_LOGGING=false

# Analysis Configuration
COLLAPSE_THRESHOLD=1.0
DEFAULT_TEMPERATURE=0.7
DEFAULT_MAX_TOKENS=500

# File Paths
PROMPTS_DATASET_PATH=prompts_dataset.csv

# Development Settings
DEBUG_MODE=false
SAVE_RAW_RESPONSES=true 